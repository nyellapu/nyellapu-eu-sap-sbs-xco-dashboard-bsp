sap.ui.define([                                                                                                                                                                                                                                                
	"sap/ui/core/mvc/Controller"                                                                                                                                                                                                                                  
], function (Controller) {                                                                                                                                                                                                                                     
	"use strict";                                                                                                                                                                                                                                                 
	                                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
	return Controller.extend("com.mckesson.eu.SBSCrossCompanySales.controller.View1", {                                                                                                                                                                           
		onInit: function () {                                                                                                                                                                                                                                        
			var that = this;                                                                                                                                                                                                                                            
			$.get("/sap/bc/bsp/chc/r_xcompany/index.htm", function (data) {                                                                                                                                                                                             
			//	console.log(data);                                                                                                                                                                                                                                       
				that.getView().byId('html').setContent(data);                                                                                                                                                                                                              
			});                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
		}                                                                                                                                                                                                                                                            
	});                                                                                                                                                                                                                                                           
});                                                                                                                                                                                                                                                            